{
  "name": "machinepack-ifthen",
  "version": "0.4.1",
  "description": "Machines for branching based on comparisons, validations, etc.",
  "scripts": {
    "test": "node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js"
  },
  "keywords": [
    "If...Then",
    "machines",
    "machinepack"
  ],
  "author": "Mike McNeil",
  "license": "MIT",
  "dependencies": {
    "lodash": "^3.9.2",
    "machine": "^9.1.2"
  },
  "devDependencies": {
    "test-machinepack-mocha": "^1.0.0"
  },
  "machinepack": {
    "friendlyName": "If...Then",
    "machineDir": "machines/",
    "machines": [
      "if-defined",
      "if-less-than",
      "if-greater-than",
      "if-equal",
      "if-between",
      "if-then-finally",
      "if-then-finally-sync"
    ],
    "testsUrl": "https://travis-ci.org/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:treelinehq/machinepack-ifthen.git"
  }
}
